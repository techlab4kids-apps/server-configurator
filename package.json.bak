{
  "name": "server-configurator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ngstart": "ng serve",
    "build": "ng build",
    "ngtest": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "webpack-dev-server --mode development --watch-content-base",
    "test": "rimraf .tmp && mocha-webpack --opts _config/mocha/mocha-webpack.opts",
    "test:watch": "rimraf .tmp && mocha-webpack --opts _config/mocha/mocha-webpack.opts --watch",
    "test:server": "webpack-dev-server --mode development --config _config/webpack.test.browser.js",
    "test:coverage": "rimraf reports && cross-env NODE_ENV=coverage nyc --reporter=text --reporter=text-summary mocha-webpack --opts _config/mocha/mocha-webpack.opts",
    "test:reports": "rimraf reports && cross-env NODE_ENV=coverage nyc mocha-webpack --opts _config/mocha/mocha-webpack.opts --reporter=xunit --reporter-options output=reports/tests/tests-report.xml",
    "tslint": "tslint --format stylish --project tsconfig.json -c tslint.json --force",
    "cy:open": "cypress open",
    "cy:run": "cypress run"

  },
